        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Start moving

//If there's a 'Up' modifier in place, move up.
if (collision_rectangle(bbox_left,bbox_top,bbox_right,bbox_bottom,obj_up,0,0))
    vspeed = -1;
    
//Otherwise, there's a 'Down' modifier in place, move down.
else if (collision_rectangle(bbox_left,bbox_top,bbox_right,bbox_bottom,obj_down,0,0))
    vspeed = 1;
</string>
          </argument>
        </arguments>
