          <argument>
            <kind>1</kind>
            <string>///Draw player

draw_sprite_ext(sprite_index,-1,round(x),round(y)+1,xscale,1,0,c_white,1);

//Set the font
draw_set_font(fnt_debug);

//Draw the debug information
draw_text_colour_shadowed(view_xview[0]+2,view_yview[0]+2,"X: " + string(x)
