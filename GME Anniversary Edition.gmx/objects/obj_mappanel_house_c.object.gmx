        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Print level name and perform actions

//If the inventory is not open...
if (obj_mapcontrol.inventory == false)

//...and the player can move...
&amp;&amp; (other.speed == 0)
&amp;&amp; (other.ready == 0)
&amp;&amp; (other.letsgo == 0)

//...and there's no openers around
&amp;&amp; (instance_number(obj_opener) == 0) {

    //If the 'Shift' key is held
    if (keyboard_check_pressed(global.shiftkey)) 
    &amp;&amp; (beaten == 0) {
    
        //Play 'Enter Stage' sound
        audio_play_sound(snd_enterstage, 0, false);
                
        //Check collected star coins
        event_user(0);
        
        //Set up the minigame
        event_user(1);
        
        //Make the player hereby the room
        with (other) {
        
            //Set up the destination room
            myroom = other.myroom;
            alarm[1] = 2;
            
            //Set the victory pose
            letsgo = true;
            
            //Make sure the player can exit the level
            global.levelbeat = other.beaten;
        }
    }
}

//Print level name on obj_mapcontrol
if (other.speed == 0)
&amp;&amp; (obj_mapcontrol.levelname = "")
    obj_mapcontrol.levelname = levelname;
</string>
