            <string>///Minigame House

/*
**  This item uses creation code!
**
**  exitdir = Regular exit direction
**  exitalt = Secret exit direction      
**      0: Up
**      1: Down
**      2: Left
**      3: Right
**
**  saveme = Whether to save the game after beating
**  myroom = Minigame to play (Add your custom ones too)
**      0: Digging Minigame
*/

//Default variables
levelname = "Minigame House"
levelid = "999";
exitdir = 0;
exitalt = 0;
saveme = 0;
myroom = 0;

//Animate
image_speed = 0;

//Whether the level has been beaten
beaten = 0;

//Whether a checkpoint was hit inside the level
checkpoint = noone;

//Check map value
if (ds_map_find_value(global.mapscreen,id) &gt; 0) {

    visible = 1;
    if (ds_map_find_value(global.mapscreen,id) == 2) {
    
        beaten = 1;
        image_index = 1;
    }
}
