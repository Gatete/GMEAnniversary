rain = part_type_create();

//Set particle sprite
part_type_sprite(rain, spr_rain, 0, 0, 1);

//Set particle alpha
part_type_alpha1(rain, 0.7);

//Set particle motion
part_type_speed(rain, 7, 15, 0, 0);
part_type_direction(rain, 225, 225, 0, 0);

//Set particle colour
part_type_colour1(rain, c_white);

//Create emitter
emitter = part_emitter_create(rain)

