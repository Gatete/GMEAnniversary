            <kind>1</kind>
            <string>///Puller logic

//If the player has died while pulling.
if (instance_exists(obj_player_dead)) {

    instance_destroy();
    exit;
}

//Snap into position
x = obj_playerparent.x;
y = obj_playerparent.y;

//Keep the player invisible
if (obj_playerparent.y &gt; ystart) {

    //With the player
    with (obj_playerparent) {
    
        //Make him invisible
        visible = false;
        
        //Stop him
        xspeed = 0;
        yspeed = 0;
        
        //Snap in place
        y = other.ystart;
        
        //Disable controls
        disablecontrol = true;
    }
}

//If the object is heavy enough and the control key is not longer held.
if (isheavy) 
&amp;&amp; (!keyboard_check(global.controlkey))
    instance_destroy();
