    if (collision_rectangle(bbox_left-hspeed*2,bbox_top,bbox_left,bbox_bottom,obj_enemyparent,0,1))
    &amp;&amp; (!collision_rectangle(bbox_left-hspeed*2,bbox_top,bbox_left,bbox_bottom,obj_wiggler_body,0,0))
        hspeed = -hspeed;
