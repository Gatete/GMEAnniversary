            <string>///Special turn at enemies script

//If the wiggler is moving right...
if (hspeed &gt; 0) {

    //...and there's a enemy on the way and that enemy is not it's body.
    if (collision_rectangle(bbox_right,bbox_top,bbox_right+hspeed*2,bbox_bottom,obj_enemyparent,0,1))
    &amp;&amp; (!collision_rectangle(bbox_right,bbox_top,bbox_right+hspeed*2,bbox_bottom,obj_wiggler_body,0,0))
        hspeed = -hspeed;
