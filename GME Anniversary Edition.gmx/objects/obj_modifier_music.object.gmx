            <string>///Change the music to play in the map

//Do not continue if the game is restarting
if (obj_persistent.restart == false) {

    if (global.world != mapworld)
    &amp;&amp; (global.clear == 0)  
    &amp;&amp; (other.speed == 0) {
        
        //Set up the world
        global.world = mapworld;
        
        //Set up the music
        with (obj_persistent_musicplayer) {
        
            //Stop map music
            alarm[3] = 1;   
            
            //Hereby map music
            mapmusic = other.mapmusic;            
            
            //Allow initialization
            mapmusicinit = false;                
                        
            //Play it again
            alarm[0] = 2;
            
            //Reduce volume
            if (global.clear &gt; 0)
                alarm[1] = 3; 
        }
    }
}
</string>
