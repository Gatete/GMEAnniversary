            <string>///Handle alpha values for HUD elements

//Health / Lives + Coins
if (global.healthmode) {

    //Health Alpha
    if (collision_rectangle(view_xview[0]+4,view_yview[0]+4,view_xview[0]+41,view_yview[0]+41,obj_playerparent,0,0)) {
    
        alpha1 -= 0.05;
        if (alpha1 &lt; 0.3)
            alpha1 = 0.3;
    }
    else {
    
        alpha1 += 0.05;
        if (alpha1 &gt; 1)
            alpha1 = 1;
    }
    
    //Lives / Coins Alpha
    if (collision_rectangle(view_xview[0]+45,view_yview[0]+4,view_xview[0]+87,view_yview[0]+41,obj_playerparent,0,0)) {
    
        alpha2 -= 0.05;
