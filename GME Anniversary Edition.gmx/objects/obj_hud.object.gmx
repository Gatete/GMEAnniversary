            <string>///Draw the HUD

//Health (Only visible if health mode is activated)
if (global.healthmode) {

    draw_sprite_ext(spr_hud_health, health, view_xview[0]+4, view_yview[0]+4, 1, 1, 0, c_white, alpha1);
}

//Set font
draw_set_font(numbers_w);

//Lives
draw_sprite_ext(spr_hud_mario, global.player, view_xview[0]+(4+(41*global.healthmode)), view_yview[0]+4, 1, 1, 0, c_white, alpha2);
draw_text_colour(view_xview[0]+(30+(41*global.healthmode)), view_yview[0]+4, string_add_zeros(lives, 2), c_white, c_white, c_white, c_white, alpha2);

//Set font
draw_set_font(numbers_g);

//Coins
draw_sprite_ext(spr_hud_coin, global.player, view_xview[0]+(4+(41*global.healthmode)), view_yview[0]+26, 1, 1, 0, c_white, alpha2);
draw_text_colour(view_xview[0]+(30+(41*global.healthmode)), view_yview[0]+26, string_add_zeros(global.coins, 2), c_white, c_white, c_white, c_white, alpha2);

//Reserve Box
draw_sprite_ext(spr_hud_reserve, -1, view_xview[0]+view_wview[0]/2, view_yview[0]+18, 1, 1, 0, c_white, alpha3);

//Set font
draw_set_font(numbers_w);

//Score
draw_text_colour(view_xview[0]+view_wview[0]-130, view_yview[0]+4, string_add_zeros(score, 9), c_white, c_white, c_white, c_white, alpha4);

//Set font
draw_set_font(numbers_g);

//Time
draw_sprite_ext(spr_hud_time, -1, view_xview[0]+view_wview[0]-45, view_yview[0]+4, 1, 1, 0, c_white, alpha5);
draw_text_colour(view_xview[0]+view_wview[0]-29, view_yview[0]+4, string_add_zeros(global.time, 3), c_white, c_white, c_white, c_white, alpha5);

//P-Meter
if (instance_exists(obj_playerparent)) { //If the player does exist.

    if (obj_mario.pmeter &gt;= 112) { //If the player is running...
    
        //...draw a filled flashing P-Meter.
        draw_sprite_ext(spr_hud_pmeter,6+flash, view_xview[0]+4, view_yview[0]+view_hview[0]-22, 1, 1, 0, c_white, alpha6);
    }
    else { //Otherwise, if the player is not running...
    
        //...draw a filling P-Meter
        draw_sprite_ext(spr_hud_pmeter, round(0+obj_playerparent.pmeter/18.6), view_xview[0]+4, view_yview[0]+view_hview[0]-22, 1, 1, 0, c_white, alpha6);        
    }
}
else
    draw_sprite_ext(spr_hud_pmeter, 0, view_xview[0]+4, view_yview[0]+view_hview[0]-22, 1, 1, 0, c_white, alpha6);    
</string>
