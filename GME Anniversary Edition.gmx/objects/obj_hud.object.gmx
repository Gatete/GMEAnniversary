          <argument>
            <kind>1</kind>
            <string>///Draw the HUD

//Disable the fog
if (instance_exists(obj_lightningsheet))
    d3d_set_fog(0, c_black, 0, 64);

//HUD (Left)
if (global.level != -1)
    draw_sprite_ext(spr_hud, 0, view_xview[0]+5, view_yview[0]+5, 1, 1, 0, c_white, 1);
else
    draw_sprite_ext(spr_hud, 1, view_xview[0]+5, view_yview[0]+5, 1, 1, 0, c_white, 1);

//Set font
draw_set_font(global.numbers_w);

//Lives
draw_text_colour_shadowed(view_xview[0]+26, view_yview[0]+10, string_format(lives, 2, 0), c_black, c_white, 1, 1, 1, 1);

//Set font
draw_set_font(global.numbers_g);

//Coins
draw_text_colour_shadowed(view_xview[0]+71, view_yview[0]+10, string_format(global.coins, 2, 0), c_black, c_white, 1, 1, 1, 1);

//Star coins
for (var i = 0; i &lt; 3; i++) {

    //Draw the coin if it has been collected
    if (ds_map_find_value(global.sc[i],global.level) &gt; 0)
        draw_sprite_ext(spr_hud_starcoin_get, -1, view_xview[0]+5+(17*i), view_yview[0]+28, 1, 1, 0, c_white, 1);
}

//Cards
draw_sprite_ext(spr_hud_cards, 0, view_xview[0]+view_wview[0]-67,view_yview[0]+view_hview[0]-24, 1, 1, 0, c_white, 1);
for (var i = 0; i &lt; 3; i++) {

    if (global.card[i] != -1)
        draw_sprite_ext(spr_goalcard, global.card[i], view_xview[0]+view_wview[0]-(66-(22*i)), view_yview[0]+view_hview[0]-23, 1, 1, 0, c_white, 1);
}

//Reserve Box / Health Meter
if (global.healthmode == 0) {

    //Box
    draw_sprite_ext(spr_hud_reserve, 0, view_xview[0]+view_wview[0]/2, view_yview[0]+19, 1, 1, 0, c_white, 1);
    
    //Reserve item
    if (global.reserve &gt; cs_small)
        draw_sprite_ext(constant_get_sprite(global.reserve), 0, view_xview[0]+view_wview[0]/2, view_yview[0]+11, 1, 1, 0, c_white, 1);
}
else {

    //Draw health
    draw_sprite_ext(spr_hud_health, health, view_xview[0]+view_wview[0]/2, view_yview[0]+23, 1, 1, 0, c_white, 1);
}

//HUD (Right)
draw_sprite_ext(spr_hud_r, 0, view_xview[0]+view_wview[0]-83, view_yview[0]+5, 1, 1, 0, c_white, 1);

//Set font
draw_set_font(global.numbers_w);

//Time
draw_text_colour_shadowed(view_xview[0]+view_wview[0]-29, view_yview[0]+10, string_add_zeros(global.time, 3), c_black, c_white, 1, 1, 1, 1);

//Score
draw_text_colour_shadowed(view_xview[0]+view_wview[0]-85, view_yview[0]+28, string_add_zeros(score, 10), c_black, c_white, 1, 1, 1, 1);

//P-Meter
if (instance_exists(obj_playerparent)) {

    if (obj_playerparent.pmeter &gt; 111)
        draw_sprite_ext(spr_hud_pmeter, 6+flash, view_xview[0]+5, view_yview[0]+view_hview[0]-20, 1, 1, 0, c_white, 1);
    else
        draw_sprite_ext(spr_hud_pmeter, 0+(obj_playerparent.pmeter/16), view_xview[0]+5, view_yview[0]+view_hview[0]-20, 1, 1, 0, c_white, 1);
}
else
    draw_sprite_ext(spr_hud_pmeter, 0, view_xview[0]+5, view_yview[0]+view_hview[0]-20, 1, 1, 0, c_white, 1);
    
//Enable the fog
if (instance_exists(obj_lightningsheet))
    d3d_set_fog(1, c_black, 0, 64);
</string>
