    }
    else {
    
        alpha2 += 0.05;
        if (alpha2 &gt; 1)
            alpha2 = 1;
    }        
}
else {

    //Lives / Coins Alpha
    if (collision_rectangle(view_xview[0]+4,view_yview[0]+4,view_xview[0]+46,view_yview[0]+41,obj_playerparent,0,0)) {
    
        alpha2 -= 0.05;
